#include "vecteur3.hpp"
#include <iostream>

using namespace std;

void afficher(const Vecteur3& v)
{
  cout << "(" << v._x << ", " << v._y << "," << v._z << ")" << endl; 
}

void Vecteur3::afficher()
{
  cout << "(" << _x << ", " << _y << "," << _z << ")" << endl; 
}

